#use $DCSDIR/include
#include <dcsddl.h>
#include <sqlDataTypes.h>
#include <dcscolwid.h>
 
#define SEQ uc_www_call_seq
#define SEED 1
#define INC 1
#define MAX 999999999999

mset command on
[
CREATE_SEQ(SEQ, SEED, INC, MAX)
] catch (-2714,-955)
RUN_DDL

[insert into sysctl (numcod, prefix, prelen, prlflg, seqlen,
         suflen, btsflg, lstseq, seqflg)
    values ('uc_www_call_seq', '', 0, 0, 12,
             0, 0, 0, 0)
] catch (-1)
RUN_DDL
mset command off

